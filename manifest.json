{
    "manifest_version": 3,
    "name": "__MSG_appName__",
    "description": "__MSG_appDescription__",
    "version": "1.2",
    "default_locale": "en",
    "permissions": [
      "downloads",
      "storage",
      "activeTab"
    ],
    "host_permissions": [
      "https://*.atlassian.net/*"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  